<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>Cadastrar Funcionário</title>
    </head>
    <body>
        <div class="container">
             
             <form role="form">
             
                <input class="form-control" placeholder="Nome do Funcionário" type="text" name="txtFuncionarioNome"><br>
                <input class="form-control" placeholder="Sobrenome" type="text" name="txtFuncionarioSobrenome"><br>
                <input class="form-control" placeholder="CPF" type="text" name="txtFuncionarioCpf"><br>
                <input class="form-control" placeholder="Data de Nascimento" type="text" name="txtFuncionarioDataNascimento"><br>
                <input class="form-control" placeholder="Telefone" type="text" name="txtFuncionarioTelefone"><br>
                <input class="form-control" placeholder="Outro Telefone" type="text" name="txtFuncionarioTelefoneAlternativo"><br>
                <input class="form-control" placeholder="Celular" type="text" name="txtFuncionarioCelular"><br>
                <input class="form-control" placeholder="E-mail" type="text" name="txtFuncionarioEmail"><br>
                <input class="form-control" placeholder="Logradouro" type="text" name="txtFuncionarioLogradoutoNome"><br>
                <input class="form-control" placeholder="Número" type="text" name="txtFuncionarioNumeroLogradouto"><br>
                <input class="form-control" placeholder="Tipo" type="text" name="txtFuncionarioLogradouroTipo"><br>
                <input class="form-control" placeholder="CEP" type="text" name="txtFuncionarioLogradouroCep"><br>
                <input class="form-control" placeholder="Bairro" type="text" name="txtFuncionarioBairroNome"><br>
                <input class="form-control" placeholder="Cidade" type="text" name="txtFuncionarioCidadeNome"><br>
                <input class="form-control" placeholder="Estado" type="text" name="txtFuncionarioEstadoNome"><br>
                <input class="form-control" placeholder="Cargo" type="text" name="txtFuncionarioCargo"><br>
                <input class="form-control" placeholder="Unidade" type="text" name="txtFuncionarioUnidade"><br>
         
             </form>
            
        </div>
    </body>
</html>
